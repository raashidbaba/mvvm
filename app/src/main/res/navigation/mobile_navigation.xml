<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_category">

    <fragment
        android:id="@+id/nav_category"
        android:name="com.example.mvvmproject.View.CategoryUI.CategoryFragment"
        android:label="@string/menu_category"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_latest"
        android:name="com.example.mvvmproject.View.LatestUI.LatestFragment"
        android:label="@string/menu_latest"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_favourite"
        android:name="com.example.mvvmproject.View.FavouriteUI.FavouriteFragment"
        android:label="@string/menu_favourite"
        tools:layout="@layout/fragment_slideshow" />
</navigation>